{
  "name": "ybx_parent_client",
  "version": "1.0.0",
  "description": "优伴学家长端app",
  "main": "mocks/index.cjs",
  "scripts": {
    "dev": "run-p flutter:dev",
    "dev:prod": "run-p flutter:prod",
    "dev:mock": "run-p mock-script",
    "build:apk": "run-p flutter:build-apk",
    "build:ios": "run-p flutter:build-ios",
    "server": "pnpm -F mocks run start",
    "mock-script": "node scripts/run-mock.js",
    "flutter:dev": "flutter run --dart-define-from-file .env.development",
    "flutter:prod": "flutter run --dart-define-from-file .env.production",
    "flutter:build-apk": "flutter build apk --dart-define-from-file .env.production --release",
    "flutter:build-ios": "flutter build ios --dart-define-from-file .env.production --release"
  },
  "author": "jerrywu001",
  "homepage": "https://github.com/jerrywu001/flutter-pro#readme",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
