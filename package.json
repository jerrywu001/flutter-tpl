{
  "name": "ybx_parent_client",
  "version": "1.0.0",
  "description": "优伴学家长端app",
  "main": "mocks/index.cjs",
  "scripts": {
    "prepare": "husky",
    "dev": "node scripts/run.js --dev",
    "dev:prod": "node scripts/run.js --prod",
    "dev:mock": "node scripts/run-mock.js",
    "build:apk": "flutter build apk --dart-define-from-file .env.production --release",
    "build:ios": "flutter build ios --dart-define-from-file .env.production --release",
    "server": "pnpm -F mocks run start",
    "generate-icons": "dart run flutter_launcher_icons",
    "flutter:dev": "flutter run --dart-define-from-file .env.development",
    "flutter:prod": "flutter run --dart-define-from-file .env.production"
  },
  "author": "jerrywu001",
  "homepage": "https://github.com/jerrywu001/flutter-pro#readme",
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "husky": "^9.1.7"
  }
}
